noinst_lib_libaddenda_nonstd_la_CPPFLAGS = $(PACKAGE_CPPFLAGS_SET)
noinst_lib_libaddenda_nonstd_la_CXXFLAGS = $(PACKAGE_CXXFLAGS_SET)
noinst_lib_libaddenda_nonstd_la_LDFLAGS = $(PACKAGE_LDFLAGS_SET)
noinst_lib_libaddenda_nonstd_la_LIBADD = -lnonstd++
noinst_lib_libaddenda_nonstd_la_SOURCES = $(addenda_nonstd_SUBSOURCES)
include addenda/nonstd/Makefrag.am
$(noinst_lib_libaddenda_nonstd_la_SOURCES) : disco_srcdir=addenda
$(noinst_lib_libaddenda_nonstd_la_SOURCES) : disco_objdir=noinst/obj
$(noinst_lib_libaddenda_nonstd_la_SOURCES) : scold_srcdir=$(disco_srcdir)
$(noinst_lib_libaddenda_nonstd_la_SOURCES) : scold_objdir=$(disco_objdir)
$(noinst_lib_libaddenda_nonstd_la_SOURCES) : noinst/obj/src/%.cpp : addenda/%.xcpp
	$(TF_V_DIS) $(call TF_XCPP2CPP, $<)
	$(TF_V_DONE) test -e $@
$(tf_noinst_mkdir)/libnonstd+temerarious-flagship.mk: $(noinst_lib_libaddenda_nonstd_la_SOURCES)
$(tf_noinst_mkdir)/libnonstd+temerarious-flagship.mk:
	$(TF_V_DEP) $(call TF_DEPENDENCIES, $^)
	$(TF_V_DONE) test -e $@
include $(tf_noinst_mkdir)/libnonstd+temerarious-flagship.mk
clean-dependencies:: ; rm -f $(tf_noinst_mkdir)/libnonstd+temerarious-flagship.mk
