#divert <hpp>
#import scold.easy.formatting.Formatter.template
#import std.basic_ostream
#import nonstd.required.Character
#import nonstd.required.Character_Traits
#import scold.easy.time.Point
namespace scold::easy::formatting {
  template<nonstd::required::Character CHAR, nonstd::required::Character_Traits TRAITS> inline auto operator<<(std::basic_ostream<CHAR, TRAITS> &, Formatter<time::Point> const &) -> std::basic_ostream<CHAR, TRAITS> &;
}
