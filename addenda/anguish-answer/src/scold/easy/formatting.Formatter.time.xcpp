#divert <ipp>
#import scold.easy.formatting.Formatter
#import sys.time.put
namespace scold::easy {
  template<nonstd::required::Character CHAR, nonstd::required::Character_Traits TRAITS> auto formatting::operator<<(std::basic_ostream<CHAR, TRAITS> &o, Formatter<time::Point> const &f) -> std::basic_ostream<CHAR, TRAITS> & {
    return o << sys::time::put(f.value, "%Y-%m-%d %H:%M:%S");
  }
}
#endiv
