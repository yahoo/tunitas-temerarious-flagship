#!/bin/bash
source ${0%/*}/library/rigging.sh || exit 255
explanation="with one unified m4 library"
if testify -I m4 -I abcd; then
    echo "OK ($explanation)"
    e=0
else
    echo "FAIL ($explanation)"
    e=1
fi
find tests/preconfigure/o.case-05.d/m{4,9} | sort
sed -e 's/^/configure-----> /' <(tail -n 10 tests/preconfigure/o.case-05.d/configure)
exit ${e:-2}
