#!/bin/bash
source ${0%/*}/library/rigging.sh || exit 255
explanation="with several additional m4 libraries"
if testify -I m4 -I apple -I banana -I cherry -I dog; then
    echo "OK ($explanation)"
    e=0
else
    echo "FAIL ($explanation)"
    e=1
fi
find tests/preconfigure/o.case-04.d/m{4,9} | sort
sed -e 's/^/configure-----> /' <(tail -n 10 tests/preconfigure/o.case-04.d/configure)
exit ${e:-2}
