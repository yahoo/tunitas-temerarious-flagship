check_bin_tf = check/bin/tf
check_bin_tf_LDADD = \
  $(lib_libapp_tf_la) \
  -lrigging-bench-module \
  $(end)
check_bin_tf_LDFLAGS = $(CHECK_LDFLAGS_SET)
$(check_bin_tf_OBJECTS): AM_CPPFLAGS+=$(AND_CHECK_CPPFLAGS_SET)
$(check_bin_tf_OBJECTS): AM_CXXFLAGS+=$(AND_CHECK_CXXFLAGS_SET)
check_bin_tf_MODULES = $(none)
check_bin_tf_SOURCES = \
  obj/src/tests/tf/c.main.cpp \
  obj/src/tests/tf/namespace.cpp \
  obj/src/tests/tf/Fixture.cpp \
  $(end)
#
$(scold_cxxdir)/tests/tf/dependencies.mk: $(check_bin_tf_SOURCES)
$(check_bin_tf_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_XCPP2CPP, $<)
	$(TF_V_DONE) test -e $@
$(scold_cxxdir)/tests/tf/dependencies.mk:
	$(TF_V_DEP) $(call TF_DEPENDENCIES, $^)
	$(TF_V_DONE) test -e $@
include $(scold_cxxdir)/tests/tf/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/tf/dependencies.mk
